{
  "plugins": [
    "node_modules/jsdoc/plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(x|doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "readme": "README.md"
  }
}